{
	"name": "plg-captcha-google",
	"description": "Google Invisible reCAPTCHA",
	"devDependencies": {
		"@babel/cli": "^7.11.6",
		"@babel/core": "^7.11.6",
		"@babel/preset-env": "^7.11.5",
		"babel-preset-minify": "^0.5.1"
	},
	"babel": {
		"presets": [
			[
				"@babel/preset-env",
				{
					"useBuiltIns": "entry",
					"corejs": {
						"version": "3.9",
						"proposals": true
					},
					"targets": {
						"chrome": "50",
						"edge": "17",
						"firefox": "44",
						"opera": "42"
					}
				}
			],
			[
				"minify",
				{
					"builtIns": false,
					"removeConsole": false
				}
			]
		],
		"comments": false,
		"sourceType": "script",
		"ignore": [
			"plugins/captcha/google/media/js/*.min.js"
		]
	},
	"dependencies": {
		"core-js": "^3.9.1"
	}
}
